{
  "name": "feedme-client",
  "version": "0.0.29",
  "description": "Feedme client library for Javascript.",
  "license": "MIT",
  "author": "Aaron G",
  "keywords": [
    "feedme"
  ],
  "main": "build/node.js",
  "jsdelivr": "bundle.js",
  "scripts": {
    "docs": "documentation build src/main.js -f html -o docs --access public --access private --access undefined --sort-order alpha",
    "build": "npm run test-src && gulp build && npm run test-build-node",
    "lint-src": "eslint src/**",
    "lint-build-tests": "eslint tests/*",
    "test": "npm run test-src",
    "test-src": "npm run lint-src && jest",
    "test-build-node": "node tests/tests.node.js",
    "test-build-browsers": "node tests/tests.browsers.js",
    "coverage": "jest --coverage",
    "coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls"
  },
  "dependencies": {
    "check-types": "^7.4.0",
    "component-emitter": "^1.2.1",
    "crypto-js": "^3.1.9-1",
    "debug": "^4.1.1",
    "feedme-util": "^0.2.0",
    "json-expressible": "^1.0.1",
    "lodash": "^4.17.4",
    "source-map-support": "^0.5.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "async": "^2.6.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "coveralls": "^3.0.3",
    "del": "^4.0.0",
    "documentation": "^9.1.1",
    "eslint": "^5.12.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-es5": "^1.3.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.1",
    "express": "^4.16.4",
    "fs-copy-file-sync": "^1.1.1",
    "ghooks": "^2.0.4",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-cli": "^2.0.1",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^3.0.2",
    "jasmine": "^3.3.1",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "js-string-escape": "^1.0.1",
    "prepend-file": "^1.3.1",
    "prettier": "^1.14.3",
    "request": "^2.88.0",
    "sauce-connect-launcher": "^1.2.4",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js}"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aarong/feedme-client.git"
  },
  "bugs": {
    "url": "https://github.com/aarong/feedme-client/issues"
  },
  "homepage": "https://github.com/aarong/feedme-client#readme",
  "engines": {
    "node": ">=6.0.0"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run build"
    }
  },
  "browserslist": [
    "> 0.25%",
    "maintained node versions",
    "not dead"
  ]
}
